{
  "name": "NativeCrypto",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:active-arch": "react-native run-android --active-arch-only",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "studio": "cd android && studio .",
    "xcode": "open ios/NativeCrypto.xcworkspace",
    "clean:watchman": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-*",
    "clean:gradle": "cd android && ./gradlew clean",
    "pod:bundle": "cd ios && bundle install && NO_FLIPPER=1 RCT_NEW_ARCH_ENABLED=1 bundle exec pod install && cd ..",
    "pod:install": "cd ios && NO_FLIPPER=1 RCT_NEW_ARCH_ENABLED=1 pod install && cd .."
  },
  "dependencies": {
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/material-top-tabs": "^6.6.14",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.4.1",
    "@shopify/flash-list": "^1.7.1",
    "@tanstack/react-query": "^5.56.2",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "i18next": "^23.15.1",
    "millify": "^6.1.0",
    "react": "18.3.1",
    "react-freeze": "^1.0.4",
    "react-i18next": "^15.0.2",
    "react-native": "0.75.3",
    "react-native-error-boundary": "^1.2.4",
    "react-native-exception-handler": "^2.10.10",
    "react-native-gesture-handler": "^2.20.0",
    "react-native-mmkv": "^3.0.2",
    "react-native-pager-view": "^6.4.1",
    "react-native-reanimated": "^3.15.3",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-screens": "^3.34.0",
    "react-native-svg": "^15.7.1",
    "react-native-tab-view": "^3.5.2",
    "react-native-unistyles": "^2.9.2",
    "react-native-vector-icons": "^10.2.0",
    "react-use-websocket": "^4.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.3",
    "@react-native/eslint-config": "0.75.3",
    "@react-native/metro-config": "0.75.3",
    "@react-native/typescript-config": "0.75.3",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-devtools": "^6.0.0",
    "react-native-react-query-devtools": "^1.3.0",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
